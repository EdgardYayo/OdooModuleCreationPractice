<?xml version="1.0" enconding="utf-8"?>
<odoo>

    <!-- El atributo web_icon aÃ±ade una imagen al modulo custom "om_hospital"-->
    <!-- Esta debe estar ubicada en la ruta  ../static/description/hospital_icon.png -->
    <menuitem id="menu_hospital_root"
              name="Hospital"
              web_icon="om_hospital, static/description/hospital_icon.png" 
              sequence="10"/> 

    <menuitem id="menu_hospital_operations"
              name="Operations"
              parent="menu_hospital_root"
              sequence="10"/> 

    <menuitem id="menu_hospital_master"
              name="Master Data"
              parent="menu_hospital_root"
              sequence="10"/> 

    <menuitem id="menu_hospital_appointment"
              name="Appointments"
              parent="menu_hospital_root"
              sequence="10"/> 

    <menuitem id="menu_appointment_wizard"
              name="Create Appointment"
              parent="menu_hospital_appointment"
              action="action_create_appointment"
              sequence="10"/> 
</odoo>